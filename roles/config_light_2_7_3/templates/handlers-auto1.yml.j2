---
# {{ ansible_managed }}
# Automatically generated file with handlers.
{% for iitem in item.value.handlers %}

- name: {{ item.key }} {{ iitem.handler }}
  {{ iitem.module }}:
{% for param in iitem.params %}
    {{ param }}
{% endfor %}
{% if iitem.conditions is defined %}
  when:
{% for condition in iitem.conditions %}
    {{ condition }}
{% endfor %}
{% endif %}
{% endfor %}

# EOF
